        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TabPane prefHeight="760.0" prefWidth="1154.0" styleClass="background-style" stylesheets="@css/styles.css" tabClosingPolicy="UNAVAILABLE">
     <tabs>
       <Tab fx:id="tabArtist" style="-fx-border-color: CE6900BC;" text="Artist">
